{
  "name": "typedoc-plugin-markdown",
  "version": "0.0.16",
  "description": "A plugin for Typedoc that exposes themes and options for rendering markdown.",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "compileTests": "npm run compileTests:md && npm run compileTests:mdSingleFile",
    "compileTests:md": "typedoc --tsconfig tests/src/tsconfig.json --options tests/config.markdown.json",
    "compileTests:mdSingleFile": "typedoc --tsconfig  tests/src/tsconfig.json --options tests/config.markdown.singlePage.json",
    "copy:hbs": "cpx src/**/*.hbs dist/",
    "copy:pkg": "copyfiles package.json dist/index.js dist/**/*.hbs dist/**/*.js dist/theme/helpers/*.js node_modules/typedoc-plugin-markdown",
    "clean": "rm -rf dist && rm -rf test/out",
    "lint": "tslint --type-check --project tsconfig.json",
    "prepare": "tsc && npm run copy:hbs",
    "prepublishOnly": "npm run lint && npm run clean && npm run test",
    "pretest": "npm run prepare && npm run copy:pkg && npm run compileTests",
    "test": "mocha tests/index.js",
    "watch": "concurrently \"npm run watch:tsc\" \"npm run watch:hbs\"",
    "watch:tsc": "tsc -w",
    "watch:hbs": "onchange 'src/**/*.hbs' -- npm run copy:hbs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgreyuk/typedoc-plugin-markdown.git"
  },
  "keywords": [
    "typedocplugin",
    "typedoc"
  ],
  "bugs": {
    "url": "https://github.com/tgreyuk/typedoc-plugin-markdown/issues"
  },
  "author": "Thomas Grey",
  "license": "MIT",
  "peerDependencies": {
    "typedoc": "^0.7.0"
  },
  "devDependencies": {
    "@types/chai": "4.0.0",
    "@types/mocha": "2.2.41",
    "chai": "4.0.2",
    "chai-files": "11.4.0",
    "concurrently": "3.4.0",
    "copyfiles": "1.2.0",
    "cpx": "1.5.0",
    "mocha": "3.4.2",
    "onchange": "3.2.1",
    "tslint": "5.4.3",
    "typedoc": "0.7.1",
    "typescript": "2.3.4"
  }
}
