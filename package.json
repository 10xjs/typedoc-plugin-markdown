{
  "name": "typedoc-plugin-markdown",
  "version": "1.2.1",
  "description": "A plugin for TypeDoc that exposes a theme and additional arguments for rendering markdown.",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "tslint --project ./src/tsconfig.json",
    "build": "rimraf dist && tsc -p ./src/tsconfig.json && npm run copy:static",
    "prepare": "npm run lint && npm run test",
    "test": "jest --updateSnapshot",
    "copy:static": "copyfiles --up 1 ./src/**/*.hbs ./dist/",
    "examples": "npm run build && npm-run-all examples:*",
    "examples:markdown": "ts-node ./tasks/examples.ts -t markdown",
    "examples:docusaurus": "ts-node ./tasks/examples.ts -t docusaurus",
    "examples:gitbook": "ts-node ./tasks/examples.ts -t gitbook",
    "examples:html": "ts-node ./tasks/examples.ts -t default"
  },
  "author": "Thomas Grey",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "bugs": {
    "url": "https://github.com/tgreyuk/typedoc-plugin-markdown/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgreyuk/typedoc-plugin-markdown.git"
  },
  "keywords": [
    "markdown",
    "typescript",
    "typedocplugin",
    "typedoc"
  ],
  "peerDependencies": {
    "typedoc": ">=0.10.0"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^24.0.13",
    "arg": "^4.1.0",
    "chalk": "^2.4.2",
    "copyfiles": "^2.1.0",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typedoc": "^0.14.2",
    "typescript": "3.4.x"
  },
  "dependencies": {}
}
